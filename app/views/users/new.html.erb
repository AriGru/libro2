<h1>L I B R O</h1>

<h2>Sign up</h2>

<%= form_for @user, html: { multipart: true } do |f| %>

<div class="sign-up">
<p>
 <%= f.label :image %>
 <%= f.file_field :image %>
</p>

<p>
 <%= f.label :name %>
 <%= f.text_field :name %>
</p>

<p>
 <%= f.label :bio %>
 <%= f.text_field :bio %>
</p>

<p>
 <%= f.label :username %>
 <%= f.text_field :username %>
</p>
<p>
 <%= f.label :password %>
 <%= f.password_field :password %>
</p>
<p>
 <%= f.label :password_confirmation %>
 <%= f.password_field :password_confirmation %>
</p>
<p>
 <%= f.submit %>
</p>
<% end %>
